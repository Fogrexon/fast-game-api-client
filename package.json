{
  "name": "@fogrexon/fast-game-api-client",
  "version": "1.2.7",
  "description": "Fast game api client",
  "main": "build/commonjs/src/index.js",
  "module": "build/es/src/index.js",
  "scripts": {
    "build": "rollup --config --bundleConfigAsCjs",
    "start": "rollup --config --bundleConfigAsCjs --watch",
    "start:example": "http-server",
    "lint:prettier": "prettier \"./src/**/*.ts\"",
    "lint:eslint": "eslint \"./src/**/*.ts\"",
    "lint:fix": "prettier \"./src/**/*.ts\" --write & eslint \"./src/**/*.ts\" --fix",
    "docs": "typedoc --out ./docs/ ./src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fogrexon/fast-game-api-client.git"
  },
  "keywords": [
    "shader",
    "image-effect"
  ],
  "author": "Fogrexon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Fogrexon/fast-game-api-client/issues"
  },
  "homepage": "https://github.com/Fogrexon/fast-game-api-client#readme",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-typescript": "^11.1.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "rollup": "^3.20.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.5.0",
    "typedoc": "^0.23.28",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "@types/path-browserify": "^1.0.0",
    "node-fetch": "^3.3.1"
  }
